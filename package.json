{
    "name": "apple-guardian",
    "description": "A fast-paced typing practice game, your keyboard skills are the key to saving delicious apples from hungry pests!",
    "version": "1.0.3",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/vicalloy/apple-guardian.git"
    },
    "author": "vicalloy <zbirder@gmail.com> (https://haoluobo.com/)",
    "license": "MIT",
    "licenseUrl": "http://www.opensource.org/licenses/mit-license.php",
    "homepage": "https://github.com/vicalloy/apple-guardian",
    "keywords": [
        "phaser",
        "phaser3",
        "react",
        "vite",
        "typescript",
        "typing practice"
    ],
    "scripts": {
        "dev": "node log.js dev & vite --config vite/config.dev.mjs",
        "build": "node log.js build & vite build --config vite/config.prod.mjs",
        "dev-nolog": "vite --config vite/config.dev.mjs",
        "build-nolog": "vite build --config vite/config.prod.mjs"
    },
    "dependencies": {
        "phaser": "^3.88.2",
        "react": "^18.2.0",
        "react-dom": "^18.2.0"
    },
    "devDependencies": {
        "@types/react": "^18.3.3",
        "@types/react-dom": "^18.3.0",
        "@typescript-eslint/eslint-plugin": "^7.13.1",
        "@typescript-eslint/parser": "^7.13.1",
        "@vitejs/plugin-react": "^4.3.1",
        "eslint": "^8.57.0",
        "eslint-plugin-react-hooks": "^4.6.2",
        "eslint-plugin-react-refresh": "^0.4.7",
        "terser": "^5.28.1",
        "typescript": "^5.2.2",
        "vite": "^5.3.1"
    }
}